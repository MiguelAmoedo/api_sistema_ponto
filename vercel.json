{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/cliente/(.*)", "dest": "/routes/cliente.js" },
    { "src": "/funcionario/(.*)", "dest": "/routes/funcionario.js" },
    { "src": "/chamado/(.*)", "dest": "/routes/chamado.js" },
    { "src": "/ponto/(.*)", "dest": "/routes/ponto.js" },
    { "src": "/(.*)", "dest": "/app.js" }
  ]
}
